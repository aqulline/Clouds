    MDScreen:
        name:'invoice'
        FloatLayout:
            orientation:'vertical'
            Invoice:
                pos_hint:{"center_y":.9}
            RowsCard:
                pos_hint:{"center_y":.5, "center_x":.5}
                name:"2020/2023"
                icon:"cash-multiple"

    MDScreen:
        name:'log'
        FloatLayout:
            orientation:'vertical'
            MDLabel:
                bold:True
                text:'LOGIN TO INVENTORY'
                halign:"center"
                font_style:'H6'
                pos_hint:{'center_x':.5, 'center_y':.85}
                theme_text_color: "Custom"
                text_color: 83 / 225, 186 / 225, 115 / 225, 1
                height: self.texture_size[1]
                width: self.texture_size[1]
            NumberField:
                id:phone_number
                mode: "rectangle"
                fill_color: 0, 0, 0, .4
                size_hint: .8, .1
                hint_text:"Phone number:"
                pos_hint:{'center_x':.5, 'center_y':.73}
                text_color: 0, 0, 0, 1
                font_size: dp(25)
            MDTextField:
                id:log_password
                mode: "rectangle"
                fill_color: 0, 0, 0, .4
                size_hint: .8, .1
                pos_hint:{'center_x':.5, 'center_y':.58}
                hint_text:"Password:"
                password: True
                text_color: 0, 0, 0, 1
                font_size: dp(25)
            MDFillRoundFlatButton:
                id:register
                text: "Login"
                size_hint: .5, .06
                pos_hint:{'center_x':.5, 'center_y':.42}
                md_bg_color: 83 / 225, 186 / 225, 115 / 225, 1
                on_release:
                    app.register_user(phone_number.text, log_password.text)
